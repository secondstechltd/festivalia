<%- include('partials/header'); -%>
<main id="tickets">
  <%- include('partials/nav'); -%>
  <div class="main-content">
    <div class="section-wrapper wf-section">
      <h1 class="heading-7 center-align f-size-xlrg f-text-color-white">
        Make Reservation
      </h1>
      <div id="w-node-_98146d71-9b04-9569-77b1-832a4e83f680-8895db6f">
        <div class="form-block">
          <form id="reservation-form">
            <label for="name" class="f-text-color-white">Name</label
            ><input
              type="text"
              class="text-field w-input"
              maxlength="256"
              name="name"
              placeholder="Full Names"
              id="name"
            />
            <label for="email" class="f-text-color-white">Email Address</label
            ><input
              type="email"
              class="text-field-2 w-input"
              maxlength="256"
              name="email"
              placeholder="Email"
              id="email"
              required=""
            /><label for="Phone" class="f-text-color-white">Phone </label
            ><input
              type="tel"
              class="text-field-3 w-input"
              maxlength="256"
              name="phone"
              placeholder="Tel:"
              id="Phone"
              required=""
            /><input
              type="submit"
              value="Submit"
              class="submit-button w-button"
            />
            <div class="msg-wrappers">
              <div class="error-msg-wrapper">
                <p></p>
              </div>
              <div class="success-msg-wrapper">
                <p></p>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=63888341ff628b9aa72096f3"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <script
      src="https://uploads-ssl.webflow.com/63888341ff628b9aa72096f3/js/webflow.f6216faba.js"
      type="text/javascript"
    ></script>
    <!--[if lte IE 9
      ]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script
    ><![endif]-->
  </div>
</main>
<script src="/js/reservation.js"></script>
<script src="/js/main.js"></script>
<%- include('partials/footer'); -%>
